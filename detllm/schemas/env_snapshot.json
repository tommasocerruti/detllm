{
  "description": "Stable schema. Only additive changes within the same major version.",
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "detLLM EnvSnapshot",
  "type": "object",
  "required": [
    "schema_version",
    "detllm_version",
    "artifact_type",
    "python",
    "platform",
    "torch",
    "transformers",
    "device",
    "env_vars",
    "fingerprint"
  ],
  "properties": {
    "schema_version": {"type": "string"},
    "detllm_version": {"type": "string"},
    "artifact_type": {"const": "env_snapshot"},
    "fingerprint": {"type": "string"},
    "python": {"type": "object"},
    "platform": {"type": "object"},
    "torch": {"type": "object"},
    "transformers": {"type": "object"},
    "device": {"type": ["object", "null"]},
    "env_vars": {"type": "object"}
  },
  "additionalProperties": true
}
