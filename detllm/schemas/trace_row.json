{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "detLLM TokenTraceRow",
  "type": "object",
  "required": ["prompt_id", "generated_token_ids"],
  "properties": {
    "prompt_id": {"type": "string"},
    "input_token_ids": {"type": "array", "items": {"type": "integer"}},
    "input_token_ids_hash": {"type": "string"},
    "generated_token_ids": {"type": "array", "items": {"type": "integer"}},
    "scores": {"type": ["array", "null"], "items": {"type": "number"}},
    "tokenizer_id": {"type": ["string", "null"]},
    "decoding_max_new_tokens": {"type": ["integer", "null"]},
    "decoding_do_sample": {"type": ["boolean", "null"]}
  },
  "additionalProperties": true
}
